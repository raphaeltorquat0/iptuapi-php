{
    "name": "raphaeltorquat0/iptuapi-php",
    "description": "SDK oficial para a IPTU API - Dados de IPTU de Sao Paulo, Belo Horizonte e Recife",
    "type": "library",
    "keywords": [
        "iptu",
        "api",
        "sao paulo",
        "belo horizonte",
        "recife",
        "imoveis",
        "dados",
        "real estate",
        "property",
        "sdk"
    ],
    "homepage": "https://iptuapi.com.br",
    "license": "MIT",
    "authors": [
        {
            "name": "IPTU API",
            "email": "contato@iptuapi.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/raphaeltorquat0/iptuapi-php/issues",
        "source": "https://github.com/raphaeltorquat0/iptuapi-php"
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "IPTUAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPTUAPI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text",
        "phpstan": "phpstan analyse src -l 8",
        "phpcs": "phpcs --standard=PSR12 src tests",
        "phpcbf": "phpcbf --standard=PSR12 src tests"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
