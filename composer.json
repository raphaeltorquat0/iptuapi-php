{
    "name": "raphaeltorquat0/iptuapi-php",
    "description": "SDK oficial para a IPTU API - Dados de IPTU de São Paulo, Belo Horizonte, Rio de Janeiro e outras cidades",
    "version": "2.1.2",
    "type": "library",
    "license": "proprietary",
    "keywords": [
        "iptu",
        "api",
        "são paulo",
        "belo horizonte",
        "imóveis",
        "dados"
    ],
    "authors": [
        {
            "name": "IPTU API",
            "email": "contato@iptuapi.com.br"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IPTUAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPTUAPI\\Tests\\": "tests/"
        }
    },
    "homepage": "https://iptuapi.com.br",
    "support": {
        "email": "contato@iptuapi.com.br",
        "docs": "https://iptuapi.com.br/docs"
    }
}
